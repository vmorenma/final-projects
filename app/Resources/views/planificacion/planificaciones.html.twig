{% extends '::base.html.twig' %}

{% block body %}
    <h1>Planificaciones</h1>
    {% for planificacion in planificaciones %}
        <div>
            <p>{{ planificacion.nombre}}</p>
            <p>{{ planificacion.descripcion}}</p>
            <p>{{ planificacion.categoria}}</p>
        </div>
    {% endfor %}
    <div style="height: 200px"></div>
{% endblock %}